N, M = map(int, input().split())

for i in range(N // 2):
    repeats = 2 * i + 1
    pattern = ".|." * repeats
    print(pattern.center(M, '-'))

print("WELCOME".center(M, '-'))

for i in reversed(range(N // 2)):
    repeats = 2 * i + 1
    pattern = ".|." * repeats
    print(pattern.center(M, '-'))
